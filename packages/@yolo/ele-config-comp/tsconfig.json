{
  "compilerOptions": {
    "target": "es2015",
    "lib": ["esnext", "dom"],
    "module": "ESNext",
    "moduleResolution": "node",
    "baseUrl": ".",
    "types": [
    ],
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    //添加 将输出结构重定向到目录
    "paths": {
      "@/*": ["./*"]
    },
  }
}